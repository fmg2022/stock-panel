	<div class="flex items-center justify-center">
		<h1 class="mt-3 p-5 text-3xl text-slate-800 font-bold">Accesorios</h1>
	</div>
	<main class="mx-4 flex flex-col gap-8">
		<SearchBar />
		<AccesoryTable client:only="react" />
	</main>
</Layout>
